{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }

  .adventure-features-wrapper {
    background-color: {{ section.settings.bg_color | default: '#E8A668' }};
    position: relative;
    overflow: hidden;
    padding: 80px 0;
  }

  /* CUSTOM DECORATIVE ICONS - Desktop positioning */
  .adventure-features-wrapper .custom-icon-1 {
    position: absolute !important;
    top: {{ section.settings.icon1_top_desktop | default: 12 }}% !important;
    left: {{ section.settings.icon1_left_desktop | default: 4 }}% !important;
    width: {{ section.settings.icon1_size_desktop | default: 60 }}px !important;
    z-index: 1;
    opacity: 0.9;
    right: auto !important;
    bottom: auto !important;
  }

  .adventure-features-wrapper .custom-icon-2 {
    position: absolute !important;
    top: {{ section.settings.icon2_top_desktop | default: 8 }}% !important;
    right: {{ section.settings.icon2_right_desktop | default: 5 }}% !important;
    width: {{ section.settings.icon2_size_desktop | default: 200 }}px !important;
    z-index: 1;
    opacity: 0.9;
    left: auto !important;
    bottom: auto !important;
  }

  .adventure-features-wrapper .custom-icon-3 {
    position: absolute !important;
    bottom: {{ section.settings.icon3_bottom_desktop | default: 40 }}% !important;
    right: {{ section.settings.icon3_right_desktop | default: 5 }}% !important;
    width: {{ section.settings.icon3_size_desktop | default: 50 }}px !important;
    z-index: 1;
    opacity: 0.9;
    top: auto !important;
    left: auto !important;
  }

  .adventure-features-wrapper .custom-icon-4 {
    position: absolute !important;
    bottom: {{ section.settings.icon4_bottom_desktop | default: 6 }}% !important;
    left: {{ section.settings.icon4_left_desktop | default: 3 }}% !important;
    width: {{ section.settings.icon4_size_desktop | default: 160 }}px !important;
    z-index: 1;
    opacity: 0.9;
    top: auto !important;
    right: auto !important;
  }

  .adventure-features-wrapper .custom-icon-5 {
    position: absolute !important;
    top: {{ section.settings.icon5_top_desktop | default: 6 }}% !important;
    right: {{ section.settings.icon5_right_desktop | default: 2 }}% !important;
    width: {{ section.settings.icon5_size_desktop | default: 100 }}px !important;
    z-index: 2;
    opacity: 0.9;
    left: auto !important;
    bottom: auto !important;
  }

  /* Original Decorative Elements (keeping as fallback) */
  .star-top-left {
    position: absolute;
    top: 12%;
    left: 4%;
    width: 60px;
    height: 60px;
    background: #4ECDC4;
    clip-path: polygon(50% 0%, 61% 35%, 98% 35%, 68% 57%, 79% 91%, 50% 70%, 21% 91%, 32% 57%, 2% 35%, 39% 35%);
    z-index: 1;
  }

  .star-bottom-right {
    position: absolute;
    bottom: 40%;
    right: 5%;
    width: 50px;
    height: 50px;
    background: #4ECDC4;
    clip-path: polygon(50% 0%, 61% 35%, 98% 35%, 68% 57%, 79% 91%, 50% 70%, 21% 91%, 32% 57%, 2% 35%, 39% 35%);
    z-index: 1;
  }

  .cloud-bottom-left {
    position: absolute;
    bottom: 5%;
    left: 3%;
    width: 160px;
    height: 100px;
    background: #B8E6E1;
    border-radius: 80px;
    z-index: 1;
  }

  .cloud-bottom-left::before,
  .cloud-bottom-left::after {
    content: '';
    position: absolute;
    background: #B8E6E1;
    border-radius: 50%;
  }

  .cloud-bottom-left::before {
    width: 60px;
    height: 60px;
    top: -25px;
    left: 20px;
  }

  .cloud-bottom-left::after {
    width: 70px;
    height: 70px;
    top: -30px;
    right: 30px;
  }

  .rainbow-top-right {
    position: absolute;
    top: 8%;
    right: 5%;
    width: 200px;
    height: 100px;
    background: linear-gradient(90deg, #FF6B6B 0%, #FF8E53 17%, #FF9F43 33%, #10AC84 50%, #00A8FF 67%, #9C88FF 83%, #F8B500 100%);
    border-radius: 100px 100px 0 0;
    z-index: 1;
  }

  .rainbow-top-right::after {
    content: '';
    position: absolute;
    top: 25px;
    left: 25px;
    right: 25px;
    bottom: 0;
    background: #E8A668;
    border-radius: 75px 75px 0 0;
  }

  .cloud-top-right {
    position: absolute;
    top: 6%;
    right: 2%;
    width: 100px;
    height: 60px;
    background: white;
    border-radius: 50px;
    z-index: 2;
  }

  .cloud-top-right::before,
  .cloud-top-right::after {
    content: '';
    position: absolute;
    background: white;
    border-radius: 50%;
  }

  .cloud-top-right::before {
    width: 35px;
    height: 35px;
    top: -12px;
    left: 15px;
  }

  .cloud-top-right::after {
    width: 45px;
    height: 45px;
    top: -18px;
    right: 20px;
  }

  /* MAIN LAYOUT - COMPLETELY REBUILT */
  .adventure-content-container {
    max-width: 1400px;
    margin: 0 auto;
    padding: 0 40px;
    position: relative;
    z-index: 2;
  }

  .adventure-section {
    margin-bottom: 80px;
    position: relative;
  }

  /* First Section - Image ends at 50% screen width */
  .section-one {
    display: flex;
    align-items: center;
    justify-content: flex-start;
    gap: 20px;
    padding-left: calc(50% - 520px);
  }

  .section-one .image-container {
    flex: 0 0 500px;
    height: 380px;
    border-radius: 25px;
    overflow: hidden;
    position: relative;
    z-index: 10;
  }

  .section-one .text-box {
    background: transparent;
    border: 3px dotted {{ section.settings.border_color | default: 'white' }};
    border-radius: 20px;
    padding: 30px 40px;
    width: 600px;
    margin-left: -150px;
    padding-left: 190px;
    position: relative;
    z-index: 5;
  }

  /* Second Section - Image starts at 50% screen width */
  .section-two {
    display: flex;
    align-items: center;
    justify-content: flex-end;
    gap: 20px;
    padding-right: calc(50% - 520px);
  }

  .section-two .text-box {
    background: transparent;
    border: 3px dotted {{ section.settings.border_color | default: 'white' }};
    border-radius: 20px;
    padding: 30px 40px;
    width: 600px;
    margin-right: -150px;
    padding-right: 190px;
    position: relative;
    z-index: 5;
  }

  .section-two .image-container {
    flex: 0 0 500px;
    height: 380px;
    border-radius: 25px;
    overflow: hidden;
    position: relative;
    z-index: 10;
  }

  /* Image Styles */
  .adventure-image {
    width: 100%;
    height: 100%;
    object-fit: cover;
    border-radius: 25px;
  }

  /* Cloud overlays */
  .cloud-overlay {
    position: absolute;
    background: #7FB069;
    border-radius: 50px;
    z-index: 15;
    pointer-events: none;
  }

  .section-one .cloud-overlay {
    bottom: -15px;
    right: -15px;
    width: 120px;
    height: 80px;
  }

  .section-one .cloud-overlay::before,
  .section-one .cloud-overlay::after {
    content: '';
    position: absolute;
    background: #7FB069;
    border-radius: 50%;
  }

  .section-one .cloud-overlay::before {
    width: 35px;
    height: 35px;
    top: -15px;
    left: 20px;
  }

  .section-one .cloud-overlay::after {
    width: 45px;
    height: 45px;
    top: -20px;
    right: 25px;
  }

  .section-two .cloud-overlay {
    bottom: -15px;
    left: -15px;
    width: 100px;
    height: 100px;
  }

  .section-two .cloud-overlay::before,
  .section-two .cloud-overlay::after {
    content: '';
    position: absolute;
    background: #7FB069;
    border-radius: 50%;
  }

  .section-two .cloud-overlay::before {
    width: 30px;
    height: 30px;
    top: -12px;
    left: 15px;
  }

  .section-two .cloud-overlay::after {
    width: 40px;
    height: 40px;
    top: -18px;
    right: 20px;
  }

  /* White cloud on second image */
  .section-two .cloud-top-decoration {
    position: absolute;
    top: -15px;
    left: 20%;
    width: 80px;
    height: 50px;
    background: white;
    border-radius: 40px;
    z-index: 20;
  }

  .section-two .cloud-top-decoration::before,
  .section-two .cloud-top-decoration::after {
    content: '';
    position: absolute;
    background: white;
    border-radius: 50%;
  }

  .section-two .cloud-top-decoration::before {
    width: 25px;
    height: 25px;
    top: -8px;
    left: 12px;
  }

  .section-two .cloud-top-decoration::after {
    width: 30px;
    height: 30px;
    top: -12px;
    right: 15px;
  }

  /* Typography */
  .adventure-title {
    font-size: {{ section.settings.title_size | default: 36 }}px;
    font-weight: bold;
    color: {{ section.settings.text_color | default: 'white' }};
    margin-bottom: 20px;
    line-height: 1.2;
  }

  .adventure-features-list {
    list-style: none;
    padding: 0;
    margin: 0;
  }

  .adventure-features-list li {
    color: {{ section.settings.text_color | default: 'white' }};
    font-size: {{ section.settings.text_size | default: 16 }}px;
    line-height: 1.6;
    margin-bottom: 15px;
    padding-left: 25px;
    position: relative;
  }

  .adventure-features-list li::before {
    content: 'â€¢';
    color: {{ section.settings.text_color | default: 'white' }};
    position: absolute;
    left: 0;
    top: 0;
    font-size: 20px;
    font-weight: bold;
  }

  .adventure-features-list strong {
    display: inline;
    font-weight: 600;
    margin-right: 5px;
    color: {{ section.settings.text_color | default: 'white' }};
  }

  /* Mobile Responsive */
  @media screen and (max-width: 768px) {
    .adventure-content-container {
      padding: 0 20px;
    }

    .adventure-section {
      margin-bottom: 40px;
    }

    .section-one,
    .section-two {
      flex-direction: column;
      gap: 20px;
      justify-content: center;
      align-items: center;
    }

    .section-one .image-container,
    .section-two .image-container {
      width: 100%;
      height: 300px;
      flex: none;
    }

    .section-one .text-box,
    .section-two .text-box {
      width: 100%;
      margin: 0;
      padding: 25px 30px;
    }

    .adventure-title {
      font-size: {{ section.settings.title_size | default: 36 | times: 0.8 }}px;
      text-align: center;
    }

    .adventure-features-list li {
      font-size: {{ section.settings.text_size | default: 16 | times: 0.9 }}px;
    }

    /* Mobile decorative elements - Original fallbacks */
    .star-top-left, .star-bottom-right {
      width: 40px;
      height: 40px;
    }

    .cloud-bottom-left {
      width: 100px;
      height: 60px;
    }

    .rainbow-top-right {
      width: 120px;
      height: 60px;
    }

    .cloud-top-right {
      width: 60px;
      height: 35px;
    }

    /* Mobile custom icon overrides */
    .adventure-features-wrapper .custom-icon-1 {
      position: absolute !important;
      top: {{ section.settings.icon1_top_mobile | default: 10 }}% !important;
      left: {{ section.settings.icon1_left_mobile | default: 4 }}% !important;
      width: {{ section.settings.icon1_size_mobile | default: 40 }}px !important;
      right: auto !important;
      bottom: auto !important;
    }

    .adventure-features-wrapper .custom-icon-2 {
      position: absolute !important;
      top: {{ section.settings.icon2_top_mobile | default: 8 }}% !important;
      right: {{ section.settings.icon2_right_mobile | default: 5 }}% !important;
      width: {{ section.settings.icon2_size_mobile | default: 120 }}px !important;
      left: auto !important;
      bottom: auto !important;
    }

    .adventure-features-wrapper .custom-icon-3 {
      position: absolute !important;
      bottom: {{ section.settings.icon3_bottom_mobile | default: 40 }}% !important;
      right: {{ section.settings.icon3_right_mobile | default: 5 }}% !important;
      width: {{ section.settings.icon3_size_mobile | default: 35 }}px !important;
      top: auto !important;
      left: auto !important;
    }

    .adventure-features-wrapper .custom-icon-4 {
      position: absolute !important;
      bottom: {{ section.settings.icon4_bottom_mobile | default: 6 }}% !important;
      left: {{ section.settings.icon4_left_mobile | default: 3 }}% !important;
      width: {{ section.settings.icon4_size_mobile | default: 100 }}px !important;
      top: auto !important;
      right: auto !important;
    }

    .adventure-features-wrapper .custom-icon-5 {
      position: absolute !important;
      top: {{ section.settings.icon5_top_mobile | default: 6 }}% !important;
      right: {{ section.settings.icon5_right_mobile | default: 2 }}% !important;
      width: {{ section.settings.icon5_size_mobile | default: 60 }}px !important;
      left: auto !important;
      bottom: auto !important;
    }
  }
{%- endstyle -%}

<div class="section-{{ section.id }}-padding adventure-features-wrapper">
  {% if section.settings.custom_icon_1 != blank %}
    <img
      src="{{ section.settings.custom_icon_1 | image_url: width: 200 }}"
      alt="Decorative icon 1"
      class="custom-icon-1"
      loading="lazy"
    >
  {% endif %}

  {% if section.settings.custom_icon_2 != blank %}
    <img
      src="{{ section.settings.custom_icon_2 | image_url: width: 400 }}"
      alt="Decorative icon 2"
      class="custom-icon-2"
      loading="lazy"
    >
  {% endif %}

  {% if section.settings.custom_icon_3 != blank %}
    <img
      src="{{ section.settings.custom_icon_3 | image_url: width: 200 }}"
      alt="Decorative icon 3"
      class="custom-icon-3"
      loading="lazy"
    >
  {% endif %}

  {% if section.settings.custom_icon_4 != blank %}
    <img
      src="{{ section.settings.custom_icon_4 | image_url: width: 300 }}"
      alt="Decorative icon 4"
      class="custom-icon-4"
      loading="lazy"
    >
  {% endif %}

  {% if section.settings.custom_icon_5 != blank %}
    <img
      src="{{ section.settings.custom_icon_5 | image_url: width: 200 }}"
      alt="Decorative icon 5"
      class="custom-icon-5"
      loading="lazy"
    >
  {% endif %}

  <div class="star-top-left"></div>
  <div class="star-bottom-right"></div>
  <div class="cloud-bottom-left"></div>
  <div class="rainbow-top-right"></div>
  <div class="cloud-top-right"></div>

  <div class="adventure-content-container">
    
    <div class="adventure-section section-one">
      <div class="image-container">
        {% if section.settings.image_1 != blank %}
          <img src="{{ section.settings.image_1 | image_url: width: 800 }}" 
               alt="{{ section.settings.image_1.alt | default: 'Built For Adventures' }}" 
               class="adventure-image">
        {% else %}
          <div class="adventure-image" style="background: linear-gradient(135deg, #FFE8D6 0%, #E8A668 100%); display: flex; align-items: center; justify-content: center; color: #8B4513; font-weight: bold;">
            Add Adventure Image
          </div>
        {% endif %}
        <div class="cloud-overlay"></div>
      </div>
      <div class="text-box">
        <h3 class="adventure-title">{{ section.settings.title_1 | default: 'Built For Adventures' }}</h3>
        <ul class="adventure-features-list">
          <li>
            <strong>{{ section.settings.feature_1_label | default: 'Sibling-Proof Style:' }}</strong>
            {{ section.settings.feature_1_text }}
          </li>
          <li>
            <strong>{{ section.settings.feature_2_label | default: 'Story Time, Anytime:' }}</strong>
            {{ section.settings.feature_2_text }}
          </li>
        </ul>
      </div>
    </div>

    <div class="adventure-section section-two">
      <div class="text-box">
        <h3 class="adventure-title">{{ section.settings.title_2 | default: 'Kind To All' }}</h3>
        <ul class="adventure-features-list">
          <li>
            <strong>{{ section.settings.feature_3_label | default: "Planet's Best Friend:" }}</strong>
            {{ section.settings.feature_3_text }}
          </li>
          <li>
            <strong>{{ section.settings.feature_4_label | default: 'Beyond Labels:' }}</strong>
            {{ section.settings.feature_4_text }}
          </li>
        </ul>
      </div>
      <div class="image-container">
        {% if section.settings.image_2 != blank %}
          <img src="{{ section.settings.image_2 | image_url: width: 800 }}" 
               alt="{{ section.settings.image_2.alt | default: 'Kind To All' }}" 
               class="adventure-image">
        {% else %}
          <div class="adventure-image" style="background: linear-gradient(135deg, #FFE8D6 0%, #E8A668 100%); display: flex; align-items: center; justify-content: center; color: #8B4513; font-weight: bold;">
            Add Family Image
          </div>
        {% endif %}
        <div class="cloud-overlay"></div>
        <div class="cloud-top-decoration"></div>
      </div>
    </div>

  </div>
</div>

